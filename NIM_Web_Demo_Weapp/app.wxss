page {
  color: #000;
  font-size: 30rpx;
  width: 100%;
  height: 100%;
  font-family: PingFangSC-Regular;
  background-color: #F3F4F6;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fullscreen {
  width: 100%;
  height: 100%;
}
.background-wrapper {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
button[disabled]:not([type]) {
  background-color:rgb(85,188,250);
  color: #fff;
}

cover-view {
  position: absolute;
}
