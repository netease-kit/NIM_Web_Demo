{
  "name": "NIM_WEB_HTML5_DEMO",
  "version": "1.0.0",
  "description": "Web SDK Html5 Demo",
  "main": "index.js",
  "scripts": {
    "start": "run-p -lns dev server",
    "server": "node server",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "buildcss": "postcss ./src/themes/**/*.css ./src/themes/*.css -c build/postcss.config.js -d ./dist/css",
    "build": "run-p -lns build:*",
    "build:css": "npm run buildcss --poll --watch",
    "build:js": "node build/build.webpack.js",
    "buildend": "run-p -lns buildend:*",
    "buildend:css": "npm run buildcss",
    "buildend:js": "node build/buildend.webpack.js",
    "dev": "run-s -lns clean build",
    "devbuild": "run-s -lns clean buildend",
    "prod": "cross-env NODE_ENV=production run-s -lns clean build",
    "prodend": "cross-env NODE_ENV=production run-s -lns clean buildend",
    "pack": " run-s -lns pack:build pack:copy",
    "pack:build": "npm run prodend",
    "pack:copy": "rimraf h5 && mkdir h5 && cp index.html ./h5 && cp login.html ./h5 && cp regist.html ./h5 && cp -rf dist ./h5"
  },
  "keywords": [
    "frontend",
    "postcss",
    "webpack"
  ],
  "author": "netease",
  "license": "ISC",
  "dependencies": {
    "ajv": "^4.11.5",
    "axios": "^0.15.3",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "globby": "^6.1.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "iscroll": "^5.2.0",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^4.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0",
    "rimraf": "^2.6.1",
    "vue": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.1.4",
    "vue-recyclerview": "^0.4.0",
    "vue-router": "^2.3.0",
    "vue-style-loader": "^2.0.5",
    "vue-template-compiler": "^2.2.4",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "vux": "2.7.5",
    "vux-loader": "^1.0.61",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2"
  }
}
